{
  "name": "reactron",
  "version": "0.0.1",
  "description": "Dynamic Electron & React Module Loader",
  "keywords": ["electron", "react", "dynamic", "modules"],
  "author": {
    "name": "Martin Pietschmann",
    "email": "schirkan@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/schirkan/reactron"
  },
  "bugs":{
    "url": "https://github.com/schirkan/reactron/issues"
  },
  "main": "main.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "debug": "run-p debug:react debug:electron",
    "debug:react": "npm run build-css && react-scripts-ts start",
    "debug:electron": "set NODE_ENV=development& electron .",
    "start": "set NODE_ENV=production& electron .",
    "build": "npm run build:electron && npm run build:react",
    "build:react": "npm run build-css && react-scripts-ts build",
    "build:electron": "rimraf ./dist && tsc -p ./tsconfig.electron.json",
    "watch:electron": "rimraf ./dist && tsc -p ./tsconfig.electron.json -w",
    "build-modules": "node build-modules.js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts-ts test --env=jsdom"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "classnames": "^2.2.6",
    "electron": "^2.0.10",
    "electron-shutdown-command": "0.0.8",
    "electron-store": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-markdown": "^3.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/electron-store": "^1.3.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/systemjs": "^0.20.6",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "systemjs": "^0.21.4",
    "typescript": "^3.0.3"
  }
}
