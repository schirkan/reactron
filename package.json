{
    "name": "dynamic-electron-react-module-loader",
    "version": "0.0.1",
    "author": {
        "name": "Martin Pietschmann",
        "email": "schirkan@gmail.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/schirkan/dynamic-electron-react-module-loader"
    },
    "main": "main.js",
    "proxy": "http://localhost:5000",
    "scripts": {
        "debug": "run-p debug:react debug:electron",
        "debug:react": "react-scripts-ts start",
        "debug:electron": "set NODE_ENV=development& electron .",
        "start": "set NODE_ENV=production& electron .",
        "build": "npm run build:electron && npm run build:react",
        "build:react": "react-scripts-ts build",
        "build:electron": "rimraf ./dist && tsc -p ./tsconfig.electron.json",
        "build-modules": "node build-modules.js",
        "test": "react-scripts-ts test --env=jsdom"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
        "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
        "@fortawesome/react-fontawesome": "0.1.0-11",
        "electron-store": "^2.0.0",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-scripts-ts": "2.17.0",
        "rimraf": "^2.6.2"
    },
    "devDependencies": {
        "@types/electron-store": "^1.3.0",
        "@types/express": "^4.16.0",
        "@types/jest": "^23.3.1",
        "@types/node": "^10.9.1",
        "@types/react": "^16.4.11",
        "@types/react-dom": "^16.0.7",
        "@types/react-fontawesome": "^1.6.4",
        "@types/systemjs": "^0.20.6",
        "electron": "^2.0.8",
        "npm-run-all": "^4.1.3",
        "systemjs": "^0.21.4",
        "typescript": "^3.0.1"
    }
}
